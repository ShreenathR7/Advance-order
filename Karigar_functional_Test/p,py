filter_Karigar = ['THIRUMALA', 'THIRUMALA', 'THIRUMALA', 'THIRUMALA', 'THIRUMALA', 'THIRUMALA', 'THIRUMALA', 'THIRUMALA', 'THIRUMALA', 'THIRUMALA']
unique = set(filter_Karigar)  
print(sorted(unique))  
a = unique
print(a)
extracted_string = next(iter(a))
print(extracted_string)

a = ['1', '2', '3']

# Initialize a variable to store the total
total = 0

# Iterate through the elements in the list and add them to the total
for element in a:
    total = total+int(element)
    #total += int(element)  # Convert each element to an integer before adding

# Print the total
print(total)


my_set = {'CHINNA ANNAN JEWELLERY', 'CHINNA ANNAN JEWELLERY', 'HEAD OFFICE', 'HEAD OFFICE','SRI'}

# Initialize an empty list to store the strings
strings_only = []

# Iterate through the elements in the set and add them to the list
for item in my_set:
    strings_only.append(item)
    extracted_string = strings_only[0]

print(extracted_string)

my_set = {'CHINNA ANNAN JEWELLERY', 'CHINNA ANNAN JEWELLERY', 'HEAD OFFICE', 'HEAD OFFICE','SRI'}

# Convert the set to a string and remove the curly braces
result_string = ' '.join(my_set)

print(result_string)
